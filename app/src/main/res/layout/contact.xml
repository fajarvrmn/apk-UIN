<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backk"
    tools:context="com.example.dedehikmat.uin.ContactActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/hijau"
        android:textSize="10pt"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:text="CONTACT PERSON"
        android:id="@+id/button"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/telp"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="27dp"
        android:id="@+id/imageView5"
        android:layout_below="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="082-320-333-030"
        android:textSize="10pt"
        android:textStyle="bold"
        android:textColor="@color/hitam"
        android:id="@+id/textView3"
        android:layout_above="@+id/imagesView5"
        android:layout_toRightOf="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView5"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp" />


    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/wa"
        android:id="@+id/imagesView5"
        android:layout_below="@+id/imageView5"
        android:layout_marginTop="19dp"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="082-320-333-030"
        android:textSize="10pt"
        android:textStyle="bold"
        android:textColor="@color/hitam"
        android:id="@+id/textView4"
        android:layout_alignTop="@+id/imagesView5"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

</RelativeLayout>
